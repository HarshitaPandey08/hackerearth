#include<stdio.h>

int main()
{
    int t,n,a[100],i,j,s;
    scanf("%d",&t);
    while(t>0)
    {
        scanf("%d",&n);
        for(i=0;i<n;i++)
        {
            for(s=0;s<i+1;s++)
            {
                printf("*");
            }
            for(j=0;j<(2*n)-(2*(i+1));j++)
            {
                printf("#");
            }
            for(s=0;s<i+1;s++)
            {
                printf("*");
            }
            printf("\n");
            
        }
		printf("\n");
        t--;
        
    }
}
