#include<stdio.h>
#include<string.h>

int main()
{
	char s[1000];
	int n,i;
	scanf("%s",s);
	scanf("%d",&n);
	for(i=0;s[i]!='\0';i++)
	{
		if(s[i]>=65 && s[i]<=90)
		{
			s[i]=s[i]+4;
		}
		else if(s[i]>=97 && s[i]<=122)
		{
			s[i]=s[i]+4;
		}
		else if(s[i]>=48 && s[i]<=57)
		{
			continue;
		}
		else{
			continue;
		}

	}
	printf("%s",s);

}
