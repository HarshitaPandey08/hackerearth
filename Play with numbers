#include<stdio.h>
#include<math.h>
int main()
{
	long int n,q,temp,x,y;
	scanf("%ld%ld",&n,&q);
	long int a[1000000];
	a[0]=0;
	for(int i=1;i<=n;i++)
	{

		scanf("%ld",&temp);
		a[i]=a[i-1]+temp;

	}
	a[n+1]=0;
	

	
	
	while(q>0)
	{
	    scanf("%ld%ld",&x,&y);
	    
	    printf("%ld\n",(a[y]-a[x-1])/(y-x+1));
	    q--;
	}
}
